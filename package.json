{
  "name": "users-auth",
  "version": "1.0.0",
  "description": "Microservice for user's authentication.",
  "main": "app.ts",
  "scripts": {
    "start": "nodemon --exec ts-node -r tsconfig-paths/register --project config/tsconfig.json app.ts",
    "lint": "eslint . --ext .js,.ts --config config/.eslintrc.json",
    "lint:fix": "eslint . --ext .js,.ts --config config/.eslintrc.json --fix",
    "prettier": "prettier --config config/.prettierrc --ignore-path config/.prettierignore \"app.ts\" \"src/**/*.{js,ts}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciscoArques/users-auth.git"
  },
  "author": "Gonza and Fran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FranciscoArques/users-auth/issues"
  },
  "homepage": "https://github.com/FranciscoArques/users-auth#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.1.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/firebase": "^3.2.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.1",
    "prettier": "^3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
